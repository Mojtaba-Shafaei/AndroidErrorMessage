<resources>

  <!-- Base application theme. -->
  <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/em_colorPrimary</item>
    <item name="colorPrimaryDark">@color/em_colorPrimaryDark</item>
    <item name="colorAccent">@color/em_colorAccent</item>
    <item name="android:background">#EEE</item>
  </style>

  <style name="AppTheme.DarkActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/em_colorPrimary</item>
    <item name="colorPrimaryDark">@color/em_colorPrimaryDark</item>
    <item name="colorAccent">@color/em_colorAccent</item>
    <item name="android:background">#EEE</item>
  </style>

</resources>
